{
    "name": "itpeopleperformance/kj-payment-stripe-bundle-2",
    "type": "symfony-bundle",
    "description": "Payment Bundle providing access to the Stripe API",
    "keywords": ["stripe", "payment", "Symfony2"],
    "homepage": "https://github.com/kengjin/KJPaymentStripeBundle",
    "license": "MIT",
    "version": "1.1.1",
    "authors": [
        {
            "name": "Keng Jin Chew",
            "email": "kjchew@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "jms/payment-core-bundle": "^1.0",
        "stripe/stripe-php": "*",
         "ext-mcrypt": "*",
        "paragonie/random_compat": "2.*",
        "symfony/options-resolver": "2.0.*"
    },
    "require-dev": {
        "symfony/browser-kit": "*",
        "symfony/finder": "*"
    },
    "autoload": {
        "psr-0": { "KJ\\Payment\\StripeBundle": "" }
    },
    "target-dir": "KJ/Payment/StripeBundle"
}
